{
    "builds": [{
        "src": "vercel_app",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/vercel_app/wsgi.py"
        }
    ]
}
